//ESTANDARES GEOGRAFÍA E HISTORIA
$crearestandaresGeografiahistoria="
CREATE TABLE `estandaresgeografiahistoria` (
  `id` int(6) NOT NULL,
  `curso` varchar(5) COLLATE utf8_unicode_ci NOT NULL,
  `bloque` text COLLATE utf8_unicode_ci NOT NULL,
  `criterio` text COLLATE utf8_unicode_ci NOT NULL,
  `estandar` text COLLATE utf8_unicode_ci NOT NULL,
  `prioridad` varchar(5) COLLATE utf8_unicode_ci NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;
";
mysqli_query($link,$crearestandaresGeografiahistoria)or die("NO HE PODIDO CREAR LA TABLA DE ESTANADARES DE GEOGRAFIA E HISTORIA");
